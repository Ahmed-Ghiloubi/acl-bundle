<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="projecta_acl.doctrine_subscriber" class="ProjectA\Bundle\AclBundle\EventListener\DoctrineSubscriber">
            <argument type="service" id="projecta_acl.ace.objectmanager" />
            <argument>%projecta_acl.remove_orphans%</argument>
            <tag name="doctrine.event_subscriber" />
            <tag name="doctrine_mongodb.odm.event_subscriber" />
        </service>
    </services>
</container>
